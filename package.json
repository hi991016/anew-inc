    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.1.5",
    "react-transition-group": "^4.4.5",
    "sass": "^1.83.4",
    "styled-components": "^6.1.14",
    "swiper": "^11.2.2",
    "use-immer": "^0.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-transition-group": "^4.4.12",
